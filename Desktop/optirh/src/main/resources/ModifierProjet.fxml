<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="394.0" prefWidth="301.0" spacing="10" style="-fx-padding: 20;" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.nexus.Controllers.ModifierProjetController">
    <Label style="-fx-font-size: 16; -fx-font-weight: bold;" text="Modifier le Projet" />

    <Label text="Type de projet:" />
    <TextField fx:id="typeField" />

    <Label text="Description:" />
    <TextField fx:id="descriptionField" />
   <HBox prefHeight="49.0" prefWidth="262.0">
      <children>
      
          <Label prefHeight="39.0" prefWidth="108.0" text="Statut:" />
          <ComboBox fx:id="statutComboBox" prefHeight="30.0" prefWidth="122.0" />
      </children>
   </HBox>
   <HBox prefHeight="33.0" prefWidth="262.0">
      <children>
      
          <Label prefHeight="29.0" prefWidth="112.0" text="ID Utilisateur:" />
          <TextField fx:id="utilisateurIdField" prefHeight="13.0" prefWidth="42.0" />
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
      
          <Button onAction="#handleEnregistrer" prefHeight="36.0" prefWidth="86.0" text="Enregistrer" />
         <Label prefHeight="20.0" prefWidth="20.0" />
          <Button onAction="#handleAnnuler" prefHeight="22.0" prefWidth="78.0" text="Annuler" />
      </children>
   </HBox>
</VBox>
